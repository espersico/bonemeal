            <string>///Left Right Input

    //left command
        if ((gamepad_axis_value(0, gp_axislh) &lt;= -0.75 &amp;&amp; current_time &gt;= timeStamp1) ||
            (gamepad_axis_value(0, gp_axisrh) &lt;= -0.75 &amp;&amp; current_time &gt;= timeStamp1))
        {
